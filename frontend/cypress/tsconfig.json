{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
    "types": ["cypress"],
    "lib": ["ES2020", "DOM"]
  },
  "include": [
    "cypress.config.ts",
    "cypress",
    "reporter-config.js",
    "cypress/**/*.ts"
  ]
}
